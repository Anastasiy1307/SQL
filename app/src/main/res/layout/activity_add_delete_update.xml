<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Yearr"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fon3"
    android:foregroundTint="#877F7F"
    tools:context=".Add_Delete_Update">

    <Button
        android:id="@+id/btadd"
        android:layout_width="58dp"
        android:layout_height="58dp"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="20dp"
        android:backgroundTint="#cdcdcd"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#616161"
        app:icon="@android:drawable/ic_menu_add"
        app:iconTint="#251F15"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/delete"
        app:layout_constraintHorizontal_bias="0.178"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#bcbbbb" />

    <Button
        android:id="@+id/updatebt"
        android:layout_width="58dp"
        android:layout_height="58dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="20dp"
        android:backgroundTint="#cdcdcd"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#616161"
        app:icon="@android:drawable/ic_menu_edit"
        app:iconTint="#251F15"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:rippleColor="#bcbbbb"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/delete"
        android:layout_width="58dp"
        android:layout_height="58dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#cdcdcd"
        app:icon="@android:drawable/ic_menu_delete"
        app:iconTint="#251F15"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#bcbbbb"
        app:strokeColor="#B63333"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/nazad"
        android:layout_width="58dp"
        android:layout_height="58dp"
        android:layout_marginBottom="29dp"
        android:backgroundTint="#cdcdcd"
        android:onClick="onClick"
        app:icon="@android:drawable/ic_menu_directions"
        app:iconTint="#251F15"
        app:layout_constraintBottom_toTopOf="@+id/delete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#bcbbbb" />

    <TextView
        android:id="@+id/IDD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID"
        android:textColor="#20dea4"
        android:textColorHighlight="#20dea4"
        android:textColorHint="#20dea4"
        android:textColorLink="#20dea4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.251"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.06" />

    <EditText
        android:id="@+id/editTextID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#cdcdcd"
        android:ems="10"
        android:hint="Id"
        android:inputType="textPersonName"
        android:textColor="#cdcdcd"
        android:textColorHighlight="#cdcdcd"
        android:textColorHint="#cdcdcd"
        android:textColorLink="#cdcdcd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.09" />

    <EditText
        android:id="@+id/textNAme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#cdcdcd"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        android:textColor="#cdcdcd"
        android:textColorHighlight="#cdcdcd"
        android:textColorHint="#cdcdcd"
        android:textColorLink="#cdcdcd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.244" />

    <TextView
        android:id="@+id/Namme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:textColor="#20dea4"
        android:textColorHighlight="#20dea4"
        android:textColorHint="#20dea4"
        android:textColorLink="#20dea4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.267"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209" />

    <TextView
        android:id="@+id/Yearrr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Year_birthday"
        android:textColor="#20dea4"
        android:textColorHighlight="#20dea4"
        android:textColorHint="#20dea4"
        android:textColorLink="#20dea4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.307"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#cdcdcd"
        android:ems="10"
        android:hint="Year"
        android:inputType="textPersonName"
        android:textColor="#cdcdcd"
        android:textColorHighlight="#cdcdcd"
        android:textColorHint="#cdcdcd"
        android:textColorLink="#cdcdcd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.397" />
</androidx.constraintlayout.widget.ConstraintLayout>